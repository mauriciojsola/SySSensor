@model List<SySSensor.Web.Models.Logs.LogFileViewModel>

@{
    ViewBag.Title = "SySSensor - Logs";
}

<br />
<br />
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                    <i class="fa fa-bar-chart-o fa-fw"></i> Sensor Log Files
                </h3>
            </div>
            <div class="panel-body">
                <ul>
                    @foreach (var f in Model)
                    {
                        <li>@f.Filename - c: @f.DateCreated - p: @f.ProcessDate</li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            "use strict";

        });
    </script>
}
